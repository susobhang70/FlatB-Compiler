<?xml version='1.0' encoding='UTF-8'?>
<program language='Flat-B'>
	<declarations>
		<variable name='data' size='100' type='int' />
		<variable name='i' type='int' />
		<variable name='j' type='int' />
		<variable name='sum' type='int' />
		<variable name='other' type='int' />
	</declarations>
	<code>
		<forloop>
			<init>
				<equals>
					<var_location name='i' />
					<integer value='1' />
				</equals>
			</init>
			<limit>
				<integer value='100' />
			<limit>
			<code>
				<equals>
					<var_location name='data' >
						<index>
							<var_location name='i' />
						</index>
					</var_location>
					<var_location name='i' />
				</equals>
			</code>
		</forloop>
		<equals>
			<var_location name='sum' />
			<integer value='0' />
		</equals>
		<equals>
			<var_location name='other' />
			<integer value='0' />
		</equals>
		<forloop>
			<init>
				<equals>
					<var_location name='i' />
					<integer value='1' />
				</equals>
			</init>
			<limit>
				<integer value='100' />
			<limit>
			<code>
				<equals>
					<var_location name='sum' />
					<mathexpr operation='+'>
						<var_location name='sum' />
						<var_location name='data' >
							<index>
								<var_location name='i' />
							</index>
						</var_location>
					</mathexpr>
				</equals>
				<forloop>
					<init>
						<equals>
							<var_location name='j' />
							<integer value='1' />
						</equals>
					</init>
					<limit>
						<integer value='100' />
					<limit>
					<increment>
						<integer value='1' />
					</increment>
					<code>
						<equals>
							<var_location name='sum' />
							<mathexpr operation='-'>
								<var_location name='sum' />
								<integer value='1' />
							</mathexpr>
						</equals>
					</code>
				</forloop>
			</code>
		</forloop>
		<println>
			<string value='Sum value: ' />
			<var_location name='sum' />
		</println>
		<println>
			<var_location name='other' />
		</println>
	</code>
</program>
